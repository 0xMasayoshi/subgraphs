{
  "name": "trident-minimal",
  "license": "MIT",
  "repository": {
    "url": "sushiswap/subgraphs",
    "directory": "subgraphs/trident-minimal"
  },
  "files": [
    "generated"
  ],
  "scripts": {
    "generate": "mustache ../../config/$NETWORK.js template.yaml > subgraph.yaml && mustache ../../config/$NETWORK.js src/constants/index.template.ts > src/constants/index.ts && graph codegen",
    "build": "graph build",
    "test": "graph test -r",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ sushiswap/trident",
    "create-local": "graph create --node http://localhost:8020/ sushiswap/trident",
    "remove-local": "graph remove --node http://localhost:8020/ sushiswap/trident",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 sushiswap/trident",
    "deploy-kovan": "graph deploy --product hosted-service sushiswap/trident-kovan",
    "deploy-polygon": "graph deploy --product hosted-service sushiswap/trident-polygon",
    "deploy-optimism": "graph deploy --product hosted-service matthewlilley/trident-optimism",
    "deploy-kava": "graph deploy --node https://the-graph-admin.kava.io sushiswap/trident-kava"
  },
  "devDependencies": {
    "abi": "workspace:*",
    "@graphprotocol/graph-cli": "^0.32.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "matchstick-as": "^0.5.0",
    "wabt": "1.0.24"
  }
}
